/* pages/mycode/read/read.wxss */
.container{
  display: flex;
  flex-direction: row;
  justify-content: flex-start;
  align-items: flex-start;
  width:100%;
  box-sizing: border-box;
  flex-wrap: wrap;
  margin-top: -10rpx;
  margin-bottom: 100rpx;
}
.item{
  display: flex;
  flex-direction: column;
  justify-content: flex-start;
  align-items: flex-start;
  width: 355rpx;
  min-height: 300rpx;
  background-color: #fff;
  margin: 10rpx;
  padding-bottom: 16rpx;
  border-radius: 10rpx
}

.item-img {
  width: 100%;
}
.title{
  font-size: 26rpx;
  color: rgb(48, 48, 48);
  margin-top: 10rpx; 
}
.title2{
  font-size: 20rpx;
  color: rgb(99, 98, 98);
}
.content{
  width: 700rpx;
  font-size: 26rpx;
  text-overflow:ellipsis;
  word-wrap:break-word;
}

.setting{
  display: flex;
  flex-direction: row;
  align-items: center;
  justify-content: center;
  margin-top: 10rpx;
}


.publish{
  position: fixed;
  right: 50rpx;
  bottom: 300rpx;
  z-index: 1000;
}

.search-container{
  width: 726rpx;
  height: 60rpx;
  margin: 15rpx auto;
  position: relative;
}
.search-input{
  position: absolute;
  width: 100%;
  height: 60rpx;
  background-color: #fff;
  border-radius: 8rpx;
  left: 0;
  top: 0;
  z-index: 1;
  padding-left: 9%;
  font-size: 26rpx;
  font-family: Pingfang-SC-Medium;  
  color: #999999;
  box-sizing: border-box;
  line-height: 60rpx;
  letter-spacing:2rpx;
}
.index-search-ph{
  font-family: Pingfang-SC-Medium;  
  color: #999999;

}
.search-icon{
  width: 28rpx;
  height: 28rpx;
  position: absolute;
  left: 3%;
  top: 25%;
  z-index: 2;
}
.clear-icon{
  position: absolute;
  right: 4%;
  top: 20%;
  z-index: 2;
}

.btnsRow{
  width: 100%;
  height: 60rpx;

}

.ml10{
  font-size:26rpx; 
  color: rgb(99, 98, 98);
  margin-bottom:80rpx
}

/*首页样式  */
#read-page{
  width: 100%;
  height: 100%;
  overflow: auto;
}

/*tabbar样式  */
#tabbar{
    background: url(data:image/png;base64,);
  background-size: cover;
  background-position: center center; 
  display: flex;
  flex-direction: row;
  justify-content: space-around;
  align-items: center;
  position: fixed;
  bottom: 0;
  width: 100%;
  height: 130rpx;
  z-index: 10000;
}
.item1 {
  display: flex;
  flex-direction: column;
  justify-content: flex-start;
  align-items: flex-start;
  margin: 10rpx;
}
#tabbar image{
  margin-top: 25rpx;
}
#icon-read{
  width: 55rpx;
  height: 55rpx;
  margin-left: -2rpx;
}
#icon-create{
  width: 55rpx;
  height: 55rpx;
}
#icon-my{
  width: 55rpx;
  height: 55rpx;
}
